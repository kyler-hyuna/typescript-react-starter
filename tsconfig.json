{
  "compilerOptions": {
    "sourceMap": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"],
  "compileOnSave": false
}
